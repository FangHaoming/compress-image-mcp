{
  "name": "compress-image-mcp",
  "version": "1.0.2",
  "description": "MCP server for compressing images with Tinify",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "compress-image-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "node -e \"require('esbuild').buildSync({ entryPoints: ['index.js'], bundle: true, platform: 'node', format: 'esm', outfile: 'dist/index.js', packages: 'external' })\"",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "publish:public": "npm publish --access public"
  },
  "keywords": ["mcp", "model-context-protocol", "image", "compress", "tinify"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/compress-image-mcp.git"
  },
  "homepage": "https://github.com/your-username/compress-image-mcp#readme",
  "bugs": {
    "url": "https://github.com/your-username/compress-image-mcp/issues"
  },
  "files": ["dist"],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "tinify": "^1.6.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "esbuild": "^0.24.0"
  }
}
